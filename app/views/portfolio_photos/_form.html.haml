= form_for @portfolio_photo do |f|
  -if @portfolio_photo.errors.any?
    #error_explanation
      %h2= "#{pluralize(@portfolio_photo.errors.count, "error")} prohibited this portfolio_photo from being saved:"
      %ul
        - @portfolio_photo.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :title
    = f.text_field :title
  .actions
    = f.submit 'Save'
